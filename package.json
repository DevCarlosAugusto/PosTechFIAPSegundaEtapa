{
  "name": "postechfiapsegundaetapa",
  "version": "0.1.1",
  "description": "Bem-vindo ao projeto de CRUD da FIAP! Este repositório contém o código-fonte e a configuração do Docker para rodar a aplicação em um ambiente isolado, que inclui os bancos de dados MongoDB, PostgreSQL e MySQL.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec 'npm run dev:watch'",
    "dev:watch": "npm run lint && node src/server.js",
    "jsdocs": "jsdoc /src -r",
    "lint": "eslint . --ext .js",
    "test": "vitest",
    "start": "node src/server.js"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^17.2.2",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^8.18.1",
    "morgan": "~1.9.1",
    "mysql2": "^3.14.5",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@eslint/css": "^0.11.0",
    "@eslint/js": "^9.36.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.2.0",
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "jsdoc": "^4.0.4",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "vitest": "^3.2.4"
  }
}
